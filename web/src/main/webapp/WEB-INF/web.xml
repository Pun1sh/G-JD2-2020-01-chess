<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<context-param>
		<param-name>databaseUrl</param-name>
		<param-value>MyNameIsVasya</param-value>
	</context-param>
	<display-name>Simple web</display-name>

	<servlet>
		<servlet-name>MyServlet</servlet-name>
		<servlet-class>servlet.MyServlet</servlet-class>
		<init-param>
			<param-name>initParam</param-name>
			<param-value>init param value</param-value>
		</init-param>
	</servlet>

	<servlet>
		<servlet-name>MyServlet1</servlet-name>
		<servlet-class>servlet.MyServlet1</servlet-class>
	</servlet>


	<servlet>
		<servlet-name>MyServlet2</servlet-name>
		<servlet-class>servlet.MyServlet2</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>servlet.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>MyPageServlet</servlet-name>
		<servlet-class>servlet.MyPageServlet</servlet-class>
	</servlet>

	<filter>
		<filter-name>AuthorizationFilter</filter-name>
		<filter-class>filter.AuthorizationFilter</filter-class>
	</filter>
	<servlet>
		<servlet-name>resource1</servlet-name>
		<servlet-class>servlet.Resource1</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>resource2</servlet-name>
		<servlet-class>servlet.Resource2</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>resource3</servlet-name>
		<servlet-class>servlet.Resource3</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>resource4</servlet-name>
		<servlet-class>servlet.Resource4</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>MyServlet2</servlet-name>
		<url-pattern>/2</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>MyServlet1</servlet-name>
		<url-pattern>/b</url-pattern>
	</servlet-mapping>


	<servlet-mapping>
		<servlet-name>MyServlet</servlet-name>
		<url-pattern>/g</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>MyPageServlet</servlet-name>
		<url-pattern>/myPage</url-pattern>
	</servlet-mapping>

	<filter-mapping>
		<filter-name>AuthorizationFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>

	<servlet-mapping>
		<servlet-name>resource1</servlet-name>
		<url-pattern>/r1</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>resource2</servlet-name>
		<url-pattern>/r2</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>resource3</servlet-name>
		<url-pattern>/r3</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>resource4</servlet-name>
		<url-pattern>/r4</url-pattern>
	</servlet-mapping>

</web-app>